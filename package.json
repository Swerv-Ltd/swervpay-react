{
  "name": "@swervpaydev/react",
  "version": "0.0.7",
  "description": "Official Swervpay Widget for React",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && pnpm run build:tsup",
    "build:tsup": "tsup --dts-resolve",
    "typecheck": "tsc --noEmit",
    "changeset:add": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "pnpm run build && changeset publish",
    "changeset:beta": "changeset pre enter beta",
    "changeset:normal": "changeset pre exit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "swervpay",
    "swyftpay",
    "widget",
    "react",
    "collection",
    "deposit",
    "payment",
    "swervpay-react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
